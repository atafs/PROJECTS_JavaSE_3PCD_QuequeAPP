package concurrency.threads;

public class Out implements Runnable{
	
	
	@Override
	public void run() {
		try {
			String texto;
			while((texto = leitor.nextLine()) != null) {
				//adiciona no final de todo o texto o novo texto
				textoRecebido.append(texto + "\n");
			}
		} catch(Exception x) {}
	}

}
