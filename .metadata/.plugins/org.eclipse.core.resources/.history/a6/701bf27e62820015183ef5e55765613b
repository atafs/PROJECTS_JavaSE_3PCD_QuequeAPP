package com.iscte.queque.client.queue;

import java.util.PriorityQueue;
import java.util.Queue;

public class ManageList {
	
	//ATTRIBUTES
	private Queue<ChatMessage> listMessages = new PriorityQueue<>();

	//CONSTRUCTOR
	public ManageList(Queue<ChatMessage> listMessages) {
		this.listMessages = listMessages;
	}

	//GETTERS AND SETTERS
	public Queue<ChatMessage> getListMessages() {
		return listMessages;
	}

	public void setListMessages(Queue<ChatMessage> listMessages) {
		this.listMessages = listMessages;
	}
	
	

	

}
