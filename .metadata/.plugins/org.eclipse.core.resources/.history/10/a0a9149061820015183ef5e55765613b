package com.iscte.queque.client.queue;

import java.util.Iterator;
import java.util.LinkedList;
import java.util.PriorityQueue;
import java.util.Queue;

public class MainQueue {

	public static void main(String[] args) {

		// LISTS
		Queue queueB = new PriorityQueue();

		// ADD
		queueB.add("element 1");
		queueB.add("element 2");
		queueB.add("element 3");

		// PEEK: element at head of the queue
		Object firstElement = queueB.element();
		System.out.println("firstElement=" + firstElement.toString());

		for (Object object : queueB) {
			System.out.println("object=" + object.toString());
		}
		
//		// REMOVE
		Object removeFirstElement = queueB.element();
		for (Object object : queueB) {
			System.err.println("object=" + object.toString());
		}
		
		
		// ACCESS via Iterator
//		Iterator iterator = queueA.iterator();
//		while (iterator.hasNext()) {
//			String element = (String) iterator.next();
//			System.out.println("element=" + element.toString());
//		}
//
//		// REMOVE
//		Object removeFirstElement = queueB.element();
//		System.out.println("element=" + removeFirstElement.toString());
//
//		removeFirstElement = queueB.remove();
//		Iterator iterator2 = queueB.iterator();
