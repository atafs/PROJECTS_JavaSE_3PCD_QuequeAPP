package com.iscte.queque._2concurrency.lock.synchronized_._vSync_1variable;

public class VariableSyncMain {
	
	//ATTRIBUTES
	private int count;
	
	//CONSTRUCTOR
	public VariableSyncMain() {
		this.count = 0;
	}
	
	//GETTER
	public int getCount() {
		return count;
	}

	//MAIN
	public static void main(String[] args) {
		//INSTANTIATE
		VariableSyncMain variableSync = new VariableSyncMain();
		
		//ANNONYMOUS CLASS
		Thread t1 = new Thread(new Runnable() {
			
			@Override
			public void run() {
				
			}
		})
	}
	
	



}
